{
    "name": "Monei",
    "version": "1.0.0",
    "description": "MONEI payment service provider integration cartridge",
    "engines": {
        "node": ">=4.0"
    },
    "scripts": {
        "lint": "npm run lint:css && npm run lint:js",
        "lint:css": "sgmf-scripts --lint css",
        "lint:js": "sgmf-scripts --lint js",
        "upload": "sgmf-scripts --upload",
        "uploadCartridge": "sgmf-scripts --uploadCartridge int_monei_sfra",
        "compile:js": "sgmf-scripts --compile js",
        "compile:scss": "sgmf-scripts --compile css",
        "build": "sgmf-scripts --compile js && sgmf-scripts --compile css",
        "cleanJS": "rimraf ./cartridges/**/cartridge/static/default/js",
        "cleanCSS": "rimraf ./cartridges/**/cartridge/static/default/css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MONEI/MONEI-Salesforce-Commerce-Cloud.git"
    },
    "devDependencies": {
        "autoprefixer": "^9.8.8",
        "css-loader": "^1.0.1",
        "eslint": "^3.19.0",
        "eslint-config-airbnb-base": "^5.0.3",
        "postcss-loader": "^3.0.0",
        "sass": "^1.51.0",
        "sass-loader": "^7.3.1",
        "sgmf-scripts": "^2.4.2",
        "stylelint": "^8.0.0",
        "stylelint-config-standard": "^17.0.0",
        "stylelint-scss": "^2.5.0",
        "webpack": "^4.0.0"
    },
    "packageName": "int_monei_sfra"
}
